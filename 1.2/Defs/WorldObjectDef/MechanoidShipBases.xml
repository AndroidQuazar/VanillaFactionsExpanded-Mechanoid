<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <WorldObjectDef Abstract="True" Name="VFEM_ShipBase">
    <description>A landed mechanoid ship.</description>
    <worldObjectClass>VFEMech.MechSettlement</worldObjectClass>
    <expandingIcon>true</expandingIcon>
    <texture>World/WorldObjects/MechanoidShip</texture>
    <expandingIconTexture>World/WorldObjects/Expanding/Sites/MechanoidBase</expandingIconTexture>
    <expandingIconPriority>10</expandingIconPriority>
    <canBePlayerHome>false</canBePlayerHome>
    <comps>
      <li Class="WorldObjectCompProperties_Abandon" />
      <li Class="WorldObjectCompProperties_TradeRequest" />
      <li Class="WorldObjectCompProperties_FormCaravan" />
      <li Class="WorldObjectCompProperties_TimedDetectionRaids" />
      <li Class="WorldObjectCompProperties_EnterCooldown" />
    </comps>
  </WorldObjectDef>


  <WorldObjectDef ParentName="VFEM_ShipBase">
    <defName>VFEM_Frigate</defName>
    <label>mechanoid frigate</label>
    <texture>World/WorldObjects/MechanoidShip</texture>
    <expandingIconTexture>World/WorldObjects/Expanding/Sites/MechanoidShipFrigate</expandingIconTexture>
    <description>Slightly increases mechanoid presence.</description>
    <modExtensions>
      <li Class="VFEMech.MechanoidBaseExtension">
        <raisesPresence>300</raisesPresence>
        <nameMaker>VFE_NamerShipFrigate</nameMaker>
      </li>
      <li Class="KCSG.FactionSettlement">
        <useStructureLayout>True</useStructureLayout>
        <chooseFromlayouts>
          <li>VFEM_Frigate1</li>
          <li>VFEM_Frigate2</li>
          <li>VFEM_Frigate3</li>
          <li>VFEM_Frigate4</li>
          <li>VFEM_Frigate5</li>
          <li>VFEM_Frigate6</li>
          <li>VFEM_Frigate7</li>
          <li>VFEM_Frigate8</li>
          <li>VFEM_Frigate9</li>
          <li>VFEM_Frigate10</li>
          <li>VFEM_Frigate11</li>
          <li>VFEM_Frigate12</li>
          <li>VFEM_FrigateDLC1</li>
          <li>VFEM_FrigateDLC2</li>
          <li>VFEM_FrigateDLC3</li>
          <li>VFEM_FrigateDLC4</li>
          <li>VFEM_FrigateDLC5</li>
          <li>VFEM_FrigateDLC6</li>
          <li>VFEM_FrigateDLC7</li>
          <li>VFEM_FrigateDLC8</li>
          <li>VFEM_FrigateDLC9</li>
